
let basketCost = (basket, prices) => {
    let totalCost = 0;
    for (let product in basket) {
        if (prices[product]) {
            totalCost += basket[product] * prices[product];
        }
    }
    return totalCost;
}

let basket = {
    "apple": 2,
    "banana": 3,
    "orange": 1
};

let prices = {
    "apple": 10,
    "banana": 5,
    "orange": 8
};

alert(basketCost(basket, prices));
